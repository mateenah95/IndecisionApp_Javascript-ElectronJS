{
  "name": "indecision",
  "version": "1.0.0",
  "description": "Random Choice Maker Application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=release-builds",
    "package-win": "electron-packager . indecision-app --overwrite --asar=true --platform=win32 --arch=ia32 --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Indecision\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --prune=true --out=release-builds",
    "build": "electron-packager . --out=dist/win --platform=win32 --arch=x64 --asar=true --overwrite"
  },
  "keywords": [
    "indecision"
  ],
  "author": "Mateen Ahmed Sheikh",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "electron": "^7.2.4",
    "electron-packager": "^13.1.1"
  }
}
